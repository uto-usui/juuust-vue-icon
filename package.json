{
  "name": "@uto-usui/vue-icons",
  "version": "0.0.29",
  "description": "Juuust Vue Icon, Automation workflow with Figma",
  "main": "src/index.js",
  "repository": "https://github.com/leadream/juuust-vue-icon.git",
  "scripts": {
    "generate": "sudo rm -rf src/icons && node bin/build.js",
    "currentColor": "sudo sed -i 's/ fill=\"black\"/ fill=\"currentColor\"/g' ./src/svg/*.svg",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --dest bundle --target lib --name index ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@types/svgo": "^1.3.2",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "figma-js": "^1.10.1",
    "got": "^11.3.0",
    "p-queue": "5.0.0",
    "svgo": "^1.3.2",
    "uppercamelcase": "^3.0.0"
  },
  "config": {
    "framework": "vue",
    "style": "fill"
  }
}
